{
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "jsxSingleQuote": false,
  "printWidth": 150,
  "singleQuote": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": [
        "*.html"
      ],
      "options": {
        "parser": "angular",
        "singleQuote": false,
        "bracketSameLine": true
      }
    },
    {
      "files": [
        "*.scss"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
